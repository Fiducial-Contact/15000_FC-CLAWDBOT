# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase Storage (for uploads)
NEXT_PUBLIC_SUPABASE_STORAGE_BUCKET=webchat-uploads
NEXT_PUBLIC_SUPABASE_SIGNED_URL_TTL_SECONDS=3600
NEXT_PUBLIC_CHAT_IMAGE_MAX_MB=10
NEXT_PUBLIC_CHAT_FILE_MAX_MB=50

# Clawdbot Gateway (WebSocket connection)
NEXT_PUBLIC_GATEWAY_WS_URL=ws://your-gateway-host:18789
NEXT_PUBLIC_GATEWAY_TOKEN=your-gateway-token

# Web Push Notifications (optional)
NEXT_PUBLIC_WEB_PUSH_PUBLIC_KEY=your-vapid-public-key
WEB_PUSH_PRIVATE_KEY=your-vapid-private-key
WEB_PUSH_API_TOKEN=your-api-token-for-push-relay
WEB_PUSH_SUBJECT=mailto:admin@example.com

# Push Relay Script (optional)
GATEWAY_WS_URL=ws://your-gateway-host:18789
GATEWAY_TOKEN=your-gateway-token
WEB_PUSH_SEND_URL=https://your-app.com/api/push/send
